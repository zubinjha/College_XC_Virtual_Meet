{
  "name": "xc_web_app",
  "version": "1.0.0",
  "description": "Cross Country Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --mac",
    "dist:win": "electron-builder --win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zubinjha/XC_Web_App.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zubinjha/XC_Web_App/issues"
  },
  "homepage": "https://github.com/zubinjha/XC_Web_App#readme",
  "devDependencies": {
    "electron": "^36.3.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "electron-dialog": "^2.0.0",
    "exceljs": "^4.4.0",
    "xlsx": "^0.18.5"
  },
  "build": {
    "appId": "com.zubinjha.xcwebapp",
    "productName": "XC Web App",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "renderer/**/*",
      "scraper/**/*",
      "package.json"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "build/logo.png"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        "zip"
      ],
      "icon": "build/logo.png"
    }
  }
}
